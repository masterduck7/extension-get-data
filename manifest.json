{
	"name": "Data Extension",
	"description": "Get data from view Chrome Extension",
	"version": "1.0.0",
	"manifest_version": 3,
	"permissions": ["activeTab", "tabs"],
	"host_permissions": ["*"],
	"icons": {
		"16": "assets/logo.png",
		"32": "assets/logo.png",
		"48": "assets/logo.png",
		"128": "assets/logo.png"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": "assets/logo.png"
	}
}
